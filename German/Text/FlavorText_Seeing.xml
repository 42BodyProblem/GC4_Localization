<?xml version='1.0' encoding='utf-8'?>
<FlavorTextDefs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schema/FlavorText.xsd">

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_MAIN</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>We see you. We greet you.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Nun, bist du nicht ein Anblick. Kapiert?</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MIMOT</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Hello! You look kind! Please be kind.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Sie sind neu in diesem Sektor, korrekt? Ich werde Ihnen ein einfaches Angebot machen: Unterwerfen Sie sich dem Drengin-Imperium oder bereiten Sie sich auf Vernichtung vor.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_YOR</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Ihr organisches Aussehen bricht mit unserem aktuellen Verständnis der Zusammensetzung organischer, empfindungsfähiger Leben.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Hallo, hallo. Entschuldigung für die Ablenkung; viel zu konzentrieren. Schau dich an: Kannst du uns helfen, etwas in dieser weiten Ausdehnung zu finden?</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Ah, Grüße. Ich hoffe, Sie finden ein komfortables Zuhause in diesem Teil der Galaxie. Wir wären großartige Partner, wenn Sie an einigen heimlichen Arbeiten interessiert sind.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Wir erkennen Ihren Hyperantrieb. Er ist uns bekannt.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_FESTRON</ListenerFaction>
			</Requirements>
			<Text>Wir sehen Ihre Gestalt. Wir verstehen Ihre Biologie.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Wir bemerken viele Erweiterungen und schnelle Entwicklung.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Möchten Sie mehr sehen. Wir lernen viel von Synthetikern.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Wir sehen Krieg als unvermeidlich.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>We recognize. We are familiar.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>Wir erkennen. Wir wissen, dass Sie Gefahr sind.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_RESPONSE_PEACEFUL</InternalName>

		<FlavorTextOption>
			<Requirements>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We see you. We greet you.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We do not understand.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We do not fear you.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_ALTARIAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We appreciate.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir können helfen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir werden es in Betracht ziehen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Danke, ich bin sicher, unserer ist weit überlegen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Ich erkenne Sie nicht als Bedrohung; starren Sie so viel Sie wollen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_FESTRON</ListenerFaction>
			</Requirements>
			<Text>Dann wirst du die Bedürfnisse unserer Küken verstehen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Zeit ist von höchster Bedeutung.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Du bist nicht so dumm, wie ich gehört habe.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Ah, kleiner Intueri. Willkommen zurück bei den Sternen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>[I]Wirf ein paar Augen.[/I]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_RESPONSE_NEUTRAL</InternalName>

		<FlavorTextOption>
			<Requirements>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir sind euch überdrüssig. Auf Wiedersehen.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We do not appreciate.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>You will fail.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_ALTARIAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir werden es in Betracht ziehen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir werden nicht helfen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Wir werden nicht berücksichtigen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Wie Ihr Sektor ohne unseren Hyperantrieb zurechtkam, ist ein Rätsel.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Negativ.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_DRENGIN</ListenerFaction>
			</Requirements>
			<Text>Du siehst, was ich möchte, dass du siehst.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Uns wurde Anlass gegeben, uns schnell zu entwickeln.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Du hast recht. Mach dich bereit für diesen Tag.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Ja, ich freue mich, dass du dich erinnert hast.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>[I]Rolle einen bedrohlichen Tentakel auf den Bildschirm.[/I]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TAUNT_PLAYER</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir beobachteten. Wir kamen an. Wir eroberten euren Planeten.</Text>
		</FlavorTextOption>

	</FlavorTextDef>
	
	<FlavorTextDef>
        <InternalName>DECLARE_WAR_MAIN</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir sind Ihrer überdrüssig. Wir sind im Krieg mit Ihnen.[BR][BR][COLOR=UIDefaultHighlight](Wir sind im Krieg)[/COLOR]</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>Wir beobachten lange genug. Wir vernichten dich.[BR][BR][COLOR=UIDefaultHighlight](Wir sind im Krieg)[/COLOR]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>GENERIC_GREETING</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir warten.</Text>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_REFUSE</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir stimmen nicht zu.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_PERFORMED</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir sind dankbar.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_WAITING</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir warten. Zeigen Sie uns Ihr Angebot.</Text>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>PLAYER_TRADE_COUNTER_OFFER_HEADER</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir überlegen.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>EXTRA_OPTIONS_MAIN_TEXT</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Wir sprechen?</Text>
		</FlavorTextOption>

	</FlavorTextDef>
		
</FlavorTextDefs>
