		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Potremmo aiutare..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Considereremo..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Grazie, sono sicuro che il nostro è molto superiore.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Non ti percepisco come una minaccia; osserva quanto vuoi.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_FESTRON</ListenerFaction>
			</Requirements>
			<Text>Allora capirai le esigenze dei nostri piccoli..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Il tempo è essenziale..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Non sei così stupido come ho sentito..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Ah, piccolo Intueri. Ben tornato tra le stelle..</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>[I]Fai rotolare alcuni occhi.[/I]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

