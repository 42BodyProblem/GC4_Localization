    <FlavorTextDef>
        <InternalName>PLAYER_INITIATED_TRADE_OFFER_HEADER</InternalName>
        <FlavorTextOption>
            <Requirements>
                <RelationsRange>
                    <RangeMin>7</RangeMin>
                    <RangeMax>100</RangeMax>
                </RelationsRange>
                <SpeakerRaceTrait>SlaversAbility</SpeakerRaceTrait>
            </Requirements>
            <Text>It is rare to meet someone that we believe is an equal to us. It is perplexing. Let us haggle.</Text>
            <PreferenceWeight>1000</PreferenceWeight>
        </FlavorTextOption>
    </FlavorTextDef>
    <FlavorTextDef>
        <InternalName>PLAYER_INITIATED_TRADE_OFFER_HEADER</InternalName>
        <FlavorTextOption>
            <Requirements>
                <RelationsRange>
                    <RangeMin>7</RangeMin>
                    <RangeMax>100</RangeMax>
                </RelationsRange>
                <SpeakerRaceTrait>SyntheticLife</SpeakerRaceTrait>
                <ListenerRaceTrait>CarbonBasedLife</ListenerRaceTrait>
            </Requirements>
            <Text>Despite you being some sort of animated meat, we have come to appreciate you if not your kind. Let us begin negotiations.</Text>
            <PreferenceWeight>1000</PreferenceWeight>
        </FlavorTextOption>
    </FlavorTextDef>
    <FlavorTextDef>
        <InternalName>PLAYER_INITIATED_TRADE_OFFER_HEADER</InternalName>
