<?xml version='1.0' encoding='utf-8'?>
<FlavorTextDefs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schema/FlavorText.xsd">

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_MAIN</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Te vemos. Te saludamos.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Vaya, sí que eres un espectáculo. ¿Lo pillas?</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MIMOT</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>¡Hola! ¡Pareces amable! Por favor, sé amable.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Eres nuevo en este sector, ¿verdad? Te haré una oferta simple: sométete al Imperio Drengin o prepárate para la aniquilación.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_YOR</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Tu apariencia orgánica rompe nuestro actual entendimiento de la composición de la vida orgánica sensible.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Hola, hola. Perdón por estar distraído; mucho en qué concentrarse. Mirándote a ti: ¿puedes ayudarnos a encontrar algo a través de esta vasta extensión?</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Ah, saludos. Espero que estés encontrando un cómodo hogar en esta parte de la galaxia. Seríamos grandes socios si estás interesado en algún trabajo subrepticio.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Reconocemos tu hiperimpulsor. Es familiar.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_FESTRON</ListenerFaction>
			</Requirements>
			<Text>Vemos tu forma. Entendemos tu biología.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Notamos muchas expansiones y desarrollo rápido.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Deseas ver más. Aprendemos mucho de los sintéticos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Vemos la guerra como inevitable.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Reconocemos. Estamos familiarizados.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>Reconocemos. Sabemos que eres peligro.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_RESPONSE_PEACEFUL</InternalName>

		<FlavorTextOption>
			<Requirements>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Te vemos. Te saludamos.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>No entendemos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We do not fear you.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_ALTARIAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>We appreciate.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Podemos ayudar.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Lo consideraremos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Gracias, estoy seguro de que el nuestro es mucho superior.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>No te percibo como una amenaza; mira todo lo que quieras.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_FESTRON</ListenerFaction>
			</Requirements>
			<Text>Entonces entenderás las necesidades de nuestras crías.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>El tiempo es esencial.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>No eres tan tonto como he oído.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Ah, pequeño Intueri. Bienvenido de nuevo a las estrellas.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>[I]Haz rodar unos cuantos ojos.[/I]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>FIRST_CONTACT_RESPONSE_NEUTRAL</InternalName>

		<FlavorTextOption>
			<Requirements>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Nos cansas. Adiós.</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_TERRAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>No apreciamos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRENGIN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Fracasarás.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_ALTARIAN</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>Lo consideraremos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_MANTI</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>No vamos a ayudar.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_DRATH</SpeakerFaction>
				<ListenerFaction>FACTION_SEEING</ListenerFaction>
			</Requirements>
			<Text>No consideraremos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_TERRAN</ListenerFaction>
			</Requirements>
			<Text>Cómo su sector pudo manejarse sin nuestro hiperimpulsor es un misterio.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_YOR</ListenerFaction>
			</Requirements>
			<Text>Negativo.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_DRENGIN</ListenerFaction>
			</Requirements>
			<Text>Ves lo que quiero que veas.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MANTI</ListenerFaction>
			</Requirements>
			<Text>Nos dieron motivo para desarrollarnos rápidamente.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_KORATH</ListenerFaction>
			</Requirements>
			<Text>Tienes razón. Prepárate para ese día</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_MOTH</ListenerFaction>
			</Requirements>
			<Text>Sí, me alegra que lo recordaras.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>[I]Enrosca un tentáculo amenazante en la pantalla.[/I]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TAUNT_PLAYER</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Observamos. Llegamos. Conquistamos tu planeta.</Text>
		</FlavorTextOption>

	</FlavorTextDef>
	
	<FlavorTextDef>
        <InternalName>DECLARE_WAR_MAIN</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Estamos cansados de ti. Estamos en guerra contigo.[BR][BR][COLOR=UIDefaultHighlight](Estamos en guerra)[/COLOR]</Text>
		</FlavorTextOption>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
				<ListenerFaction>FACTION_ABSOLUTE</ListenerFaction>
			</Requirements>
			<Text>Observamos el tiempo suficiente. Te erradicamos.[BR][BR][COLOR=UIDefaultHighlight](Estamos en guerra)[/COLOR]</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>GENERIC_GREETING</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Esperamos.</Text>
		</FlavorTextOption>
		
	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_REFUSE</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>We do not approve.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_PERFORMED</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Estamos agradecidos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>TRADE_OFFER_WAITING</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Esperamos. Muéstranos tu oferta.</Text>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>PLAYER_TRADE_COUNTER_OFFER_HEADER</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>Consideramos.</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>EXTRA_OPTIONS_MAIN_TEXT</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>¿Hablamos?</Text>
		</FlavorTextOption>

	</FlavorTextDef>
		
</FlavorTextDefs>
