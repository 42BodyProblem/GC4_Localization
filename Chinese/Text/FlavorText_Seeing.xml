		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>我们 考虑。</Text>
			<PreferenceWeight>50</PreferenceWeight>
		</FlavorTextOption>

	</FlavorTextDef>

	<FlavorTextDef>
		<InternalName>EXTRA_OPTIONS_MAIN_TEXT</InternalName>

		<FlavorTextOption>
			<Requirements>
				<SpeakerFaction>FACTION_SEEING</SpeakerFaction>
			</Requirements>
			<Text>我们 交谈？</Text>
		</FlavorTextOption>

	</FlavorTextDef>
		
</FlavorTextDefs>